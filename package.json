{
  "name": "cefetinder",
  "version": "1.0.0",
  "description": "Dating app for CEFET students",
  "scripts": {
    "start": "concurrently --kill-others --names \"USER,MATCH,SOCKET,GRAPHQL\" --prefix-colors \"blue,green,yellow,cyan\" \"npm run start:grpc:user-service\" \"npm run start:grpc:match-service\" \"npm run start:socket\" \"npm run start:graphql\"",
    "dev": "concurrently --kill-others --names \"USER,MATCH,SOCKET,GRAPHQL\" --prefix-colors \"blue,green,yellow,cyan\" \"npm run start:grpc:user-service\" \"npm run start:grpc:match-service\" \"npm run start:socket\" \"npm run start:graphql\"",
    "build": "tsc",
    "migrate": "node-pg-migrate",
    "proto:generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/grpc/proto --js_out=import_style=commonjs:./src/grpc/proto --grpc_out=grpc_js:./src/grpc/proto --proto_path=./src/grpc/proto ./src/grpc/proto/*.proto",
    "start:grpc:user-service": "ts-node src/user-service/presentation/grpc/server.ts",
    "start:grpc:user-service:old": "ts-node src/grpc/user/user.server.ts",
    "start:grpc:match-service": "ts-node src/grpc/match/match.server.ts",
    "start:graphql": "ts-node src/graphql/server.ts",
    "start:socket": "ts-node src/websocket/server.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/express5": "^1.1.2",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.7.15",
    "@supabase/supabase-js": "^2.39.0",
    "@types/cors": "^2.8.19",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "inversify": "^7.10.4",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-rc.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/google-protobuf": "^3.15.12",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.19.26",
    "@types/pg": "^8.10.9",
    "@types/reflect-metadata": "^0.1.0",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.13.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
